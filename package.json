{
  "name": "@donkeyclip/motorcortex-player",
  "version": "2.10.5",
  "description": "Player plugin for MotorCortex",
  "main": "dist/motorcortex-player.esm.js",
  "module": "dist/motorcortex-player.esm.js",
  "browser": "dist/motorcortex-player.umd.js",
  "author": "DonkeyClip PC (donkeyclip.com) <opensource@donkeyclip.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/donkeyclip/motorcortex-player"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint:js": "eslint -c .eslintrc.js src/**/*.js",
    "lint": "npm run lint:js",
    "lint:fix": "npm run lint:js -- --fix",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "rimraf dist && rollup -c",
    "start": "npm run build:lib && concurrently -c \"cyan.bold,magenta.bold\" \"npm:build:lib -- -w\"  \"npm:start:demo\" ",
    "start:demo": "webpack serve --config ./demo/webpack.config.js --mode=development --progress ",
    "build:demo": "webpack --mode=production --config ./demo/webpack.config.js",
    "test:prod": "npm run lint",
    "prebuild": "rimraf dist",
    "prepare": "husky install"
  },
  "keywords": [
    "motorcortex",
    "player"
  ],
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "@donkeyclip/motorcortex": ">= 7.4.0 < 10"
  },
  "devDependencies": {
    "@babel/cli": "7.17.10",
    "@babel/core": "7.18.5",
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.17.12",
    "@babel/plugin-syntax-jsx": "7.17.12",
    "@babel/plugin-transform-react-jsx": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@donkeyclip/motorcortex": "9.4.1",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.0",
    "@rollup/plugin-html": "0.2.4",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "concurrently": "7.2.2",
    "core-js": "3.23.2",
    "css-loader": "6.7.1",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "html-loader": "3.1.2",
    "husky": "8.0.1",
    "lint-staged": "13.0.2",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rollup": "2.75.7",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-import-css": "3.0.3",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-svg": "2.0.0",
    "rollup-plugin-svgo": "^2.0.0",
    "rollup-plugin-terser": "7.0.2",
    "style-loader": "3.3.1",
    "svg-inline-loader": "0.8.2",
    "svg-url-loader": "7.1.1",
    "tslib": "2.4.0",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.2"
  }
}
